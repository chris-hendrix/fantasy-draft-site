{
  "name": "fantasy-baseball-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migratedev": "npx prisma migrate dev --skip-seed",
    "migrate": "npx prisma migrate deploy && npx prisma generate",
    "syncdb": "npx prisma format && npx prisma generate && npx prisma db push",
    "resetdb": "npx prisma migrate reset && npm run syncdb",
    "up": "docker-compose -f docker-compose.yml up -d && docker-compose logs -f",
    "down": "docker-compose -f docker-compose.yml down",
    "devtest": "NODE_ENV=test && next dev",
    "jest": "NODE_ENV=test jest --config=test/jest/jest.config.ts --runInBand --forceExit --verbose --silent=false",
    "cypress:open": "cypress open --config-file test/cypress/cypress.config.ts",
    "cypress": "cypress run --config-file test/cypress/cypress.config.ts",
    "seed": "ts-node ./prisma/seed.ts",
    "unseed": "ts-node ./prisma/unseed.ts"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.10.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/validator": "^13.15.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "bcrypt": "^6.0.0",
    "classnames": "^2.5.1",
    "csvtojson": "^2.0.10",
    "cypress": "^14.5.0",
    "cypress-fail-fast": "^7.1.1",
    "daisyui": "^4.12.23",
    "downshift": "^9.0.9",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.4",
    "jest": "^30.0.2",
    "next": "^15.3.4",
    "next-auth": "^4.24.11",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.6",
    "prisma": "^6.10.1",
    "qs": "^6.14.0",
    "query-types": "^0.1.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unique-names-generator": "^4.7.1",
    "validator": "^13.15.15"
  }
}
