{
  "name": "fantasy-baseball-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migratedev": "npx prisma migrate dev --skip-seed",
    "migrate": "npx prisma migrate deploy && npx prisma generate",
    "syncdb": "npx prisma format && npx prisma generate && npx prisma db push",
    "resetdb": "npx prisma migrate reset && npm run syncdb",
    "up": "docker-compose -f docker-compose.yml up -d && docker-compose logs -f",
    "down": "docker-compose -f docker-compose.yml down",
    "devtest": "NODE_ENV=test && next dev",
    "jest": "NODE_ENV=test jest --config=test/jest/jest.config.ts --runInBand --forceExit --verbose --silent=false",
    "cypress:open": "cypress open --config-file test/cypress/cypress.config.ts",
    "cypress": "cypress run --config-file test/cypress/cypress.config.ts",
    "seed": "ts-node ./prisma/seed.ts",
    "unseed": "ts-node ./prisma/unseed.ts"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.1.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@supabase/supabase-js": "^2.47.10",
    "@tailwindcss/typography": "^0.5.15",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/qs": "^6.9.17",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/validator": "^13.12.2",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.20",
    "bcrypt": "^5.1.1",
    "classnames": "^2.5.1",
    "csvtojson": "^2.0.10",
    "cypress": "^13.17.0",
    "cypress-fail-fast": "^7.1.1",
    "daisyui": "^4.12.23",
    "downshift": "^9.0.8",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.4",
    "jest": "^29.7.0",
    "next": "^15.1.3",
    "next-auth": "^4.24.11",
    "node-mocks-http": "^1.16.2",
    "postcss": "^8.4.49",
    "prisma": "^6.1.0",
    "qs": "^6.13.1",
    "query-types": "^0.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.2.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "unique-names-generator": "^4.7.1",
    "validator": "^13.12.0"
  }
}
